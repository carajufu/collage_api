<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.ac.collage_api.campus.mapper.CampusMapMapper">

	<!-- 학과 조회 -->
	<select id="selectDeptListByUnivCode" parameterType="String" resultType="String">
        SELECT SUBJCT_NM 
          FROM SUBJCT
         WHERE UNIV_CODE = #{univCode}
         ORDER BY SUBJCT_NM
    </select>
</mapper>